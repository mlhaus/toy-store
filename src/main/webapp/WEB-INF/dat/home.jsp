      <div class="col-md-5">
        <img src="${pageContext.request.contextPath}/images/HeartGold.jpg" class="d-block w-100" style="object-fit: cover; height: 500px;" alt="Heart Gold">
      </div>
    </div>

    <hr class="featurette-divider">

